{
    "manifest_version": 2,

    "name": "Objectivity Check",
    "description": "extension that notifies the user if the website is overly emotional to determined whethr or not a website is risky for research",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Objectivity Check"
      },

      "permissions": [
        "activeTab",
        "storage",
        "tabs"
      ],

      "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js; object-src 'self'"
    }
